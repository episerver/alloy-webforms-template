<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Alloy.WebForms.Template.Search.Sources</id>
    <version>$version$</version>
    <authors>EPiServer AB</authors>
    <owners>EPiServer AB</owners>
    <projectUrl>http://www.episerver.com/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Contains source code for the Alloy Search Templates written using Webforms. Can be added to an Alloy project without search.</description>
    <dependencies>
      <dependency id="EPiServer.Search.Cms" version="[9,10)" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\NuGetSources\Models\**\Search*.pp" target="content\Models" />
    <file src="bin\NuGetSources\Views\Pages\SearchPageTemplate.*" target="content\Views\Pages" />
  </files>
</package>